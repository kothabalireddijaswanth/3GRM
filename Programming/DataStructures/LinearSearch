#include<stdio.h>
#include<conio.h>
void main()
{
	int a[100],i,n,k;
	printf("enter no of elements to be inserted");
	scan("%d"&n);
	printf("enter elements in ascending order");
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
	printf("enter the key to be searched");
	scanf("%d"&k);
	for(i=0;i<n;i++)
	{
		if(a[i]==k)
		{
			printf("%d is present at location %d",k,i);
			break; 
		}
	}
	if(i==n)
	printf("%d is present",k);
	getch();
}
